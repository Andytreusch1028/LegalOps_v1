{
  "name": "legalops-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml,css,scss}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml,css,scss}\"",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "build:fast": "next build --no-lint",
    "dev:fast": "next dev --turbopack --no-lint",
    "import-entities": "tsx scripts/import-florida-entities.ts",
    "import-dbas": "tsx scripts/import-fictitious-names.ts",
    "import-partnerships": "tsx scripts/import-general-partnerships.ts",
    "seed-services": "tsx prisma/seed-services.ts",
    "seed-test-data": "tsx scripts/seed-test-data.ts",
    "test-ai-agent": "tsx scripts/test-ai-agent.ts",
    "create-staff": "tsx scripts/create-staff-user.ts",
    "email-reminders": "tsx scripts/email-verification-reminders.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.66.0",
